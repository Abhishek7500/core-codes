<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cbc.service.entities">
    <class name="Employee" table="EMPLOYEENEXT">
    
        <id name="employeeId" column = "EMPLOYEE_ID" type="int"></id>
        
        <property name="firstName" column= "FIRST_NAME" type="string"> </property>
        
        <property name="secondName" column= "LAST_NAME" type="string">   </property>
        
         <property name="email" column= "EMAIL" type="string">   </property>
        
        </class>
</hibernate-mapping>
